                 wait(.2)
               VelocityTP(StartPosition8 + (TouchTheRoad and Vector3.new() or Vector3.new(0, -6, 0)))
 
 
               VelocityTP(StartPosition9 + (TouchTheRoad and Vector3.new() or Vector3.new(0, -6, 0)))
 
               VelocityTP(StartPosition10 + (TouchTheRoad and Vector3.new() or Vector3.new(0, -5, 0)))
 
               TP(StartPosition11 + (TouchTheRoad and Vector3.new() or Vector3.new(0, 0, 0)))
                wait(.7)
               VelocityTP(StartPosition12 + (TouchTheRoad and Vector3.new() or Vector3.new(0,-9, 0)))
 
               VelocityTP(StartPosition15 + (TouchTheRoad and Vector3.new() or Vector3.new(0,-11, 0)))
 
