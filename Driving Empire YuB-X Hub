               TP(StartPosition13 + (TouchTheRoad and Vector3.new() or Vector3.new(0, 0, 0)))
               wait(.4)
            VelocityTP(StartPosition14 + (TouchTheRoad and Vector3.new() or Vector3.new(0,-6, 0)))
                 wait(.1)
              TP(StartPosition1 + (TouchTheRoad and Vector3.new() or Vector3.new(0, -2, 0)))
 
               VelocityTP(EndPosition2 + (TouchTheRoad and Vector3.new() or Vector3.new(0, -5, 0)))
 
               VelocityTP(StartPosition3 + (TouchTheRoad and Vector3.new() or Vector3.new(0, -5, 0)))
               wait(.3)
 
