    if gameProccesedEvent then return end
    if input.KeyCode == Enum.KeyCode.P then
        wait()
    getgenv().movespam = false;
    getgenv().Autofram = false;
    getgenv().autofram1 = false;
    getgenv().Spambans = false;

end
end)
end)

